{
      "argv": [
        "python",
        "-m",
        "ipykernel_launcher",
        "-f",
        "{connection_file}"
      ],
      "display_name": "Pyspark 2",
      "language": "python",
      "env": {
        "PYSPARK_PYTHON": "/usr/bin/python3",
